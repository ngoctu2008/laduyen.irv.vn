<!-- BEGIN: main -->
<div class="camket-list">
	<div class=" row mg15 align-items-xs-center justify-content-xs-between">
		
		<!-- BEGIN: loop -->
		<div class="col-xs-24 col-sm-12 col-md-8">
			<div class="camket-item d-xs-inline-flex align-items-xs-end">
				<div class="camket-icon">
					<i class="{ROW.link}"></i>
				</div>
				<div class="camket-content">
					<div class="camket-title font-utm">
						<a href="/" title="{ROW.title}" target="{ROW.target}">{ROW.title}</a>
					</div>
					{ROW.description}
				</div>
			</div>
		</div>
		<!-- END: loop -->
	</div>
</div>
<!-- END: main -->